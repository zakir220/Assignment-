    "   \n",
    "    strings = x\n",
    "    punct = \"\"\n",
    "    for char in strings:\n",
    "        if char not in exclam :\n",
    "            punct = punct + char\n",
    "\n",
    "\n",
    "    print(punct)\n",
    "main()\n"
